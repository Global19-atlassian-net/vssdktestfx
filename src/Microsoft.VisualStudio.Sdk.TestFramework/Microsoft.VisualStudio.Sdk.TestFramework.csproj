<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net46</TargetFramework>
    <CodeAnalysisRuleSet>..\shipping.ruleset</CodeAnalysisRuleSet>
    <Description>A test library to help in writing unit and integration tests for Visual Studio extensions.</Description>
    <Authors>Andrew Arnott</Authors>
    <Company>Microsoft</Company>
    <Product>Microsoft VisualStudio SDK Test Framework</Product>
    <Copyright>Copyright © Microsoft</Copyright>
    <PackageProjectUrl>https://aka.ms/vssdktestfx</PackageProjectUrl>
    <PackageTags>VisualStudio sdk testing mocks</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="content\MockedVS.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="content\MockedVS.cs" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.ComponentModel.Composition" />
    <Reference Include="PresentationCore" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationFramework" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.VisualStudio.Threading" Version="15.8.192" />
    <PackageReference Include="Microsoft.VisualStudio.Composition" Version="15.8.98" />
    <PackageReference Include="Microsoft.VisualStudio.Shell.15.0" Version="15.9.28307" />
    <PackageReference Include="Microsoft.VisualStudio.Shell.Interop.12.0" Version="12.0.30111" />
    <PackageReference Include="Microsoft.Internal.VisualStudio.Shell.Interop.10.0.DesignTime" Version="10.0.1" />
    <PackageReference Include="Moq" Version="4.2.1510.2205" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Strings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Strings.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Strings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Strings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>
